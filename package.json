    "prepublish": "npm run build"
  },
  "sideEffects": false,
  "files": [
    "target/web/rawproto_bg.wasm",
    "target/web/rawproto.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/konsumer/rawproto-wasm.git"
  }
}
